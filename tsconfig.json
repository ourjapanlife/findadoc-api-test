{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 14",
    "extends": "@tsconfig/node14/tsconfig.json",
    "exclude": ["node_modules", "dist"],
    "compilerOptions": {
      "outDir": "dist",
      "allowJs": true,
      "experimentalDecorators": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true
    }
  }
